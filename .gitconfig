[user]
	name = huangyingw@Mldonkey
	email = huangyingw@gmail.com
[merge]
	tool = kdiff3
[diff]
	tool = kdiff3
[alias]
	ad = add
	bi = bisect
	bib = bisect bad
	big = bisect good
	bir = bisect reset
	bis = bisect start
	br = branch
	bra = branch -a
	brc = branch --contains
	ci = commit -am
	cl = clone
	cm = commit -am "n"
	co = checkout
	ch = cherry
	chp = cherry-pick
	di = diff
	fe = fetch
	in = init
	lf = ls-files
	lg = log --pretty=oneline 
	lga = log --all
	me = merge
	met = mergetool
	pl = pull
	ps = push --all
	pst = push --tags
	rc = rm --cached
	rmcr = rm --cached -r
	rs = reset
	rsh = reset --hard
	rsm = reset --mixed
	rss = reset --soft
	rt = remote
	rtu = remote update
	rtv = remote -v
	rv = revert
	sh = show
	sm = submodule
  st = status
  sta = stash
  tg = tag -l -n1
  vd = difftool
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "dev"]
	remote = origin
	merge = refs/heads/dev
[branch "demo"]
	remote = origin
	merge = refs/heads/demo
[branch "fix"]
	remote = origin
	merge = refs/heads/fix
[core]
	excludesfile = /root/.gitignore
