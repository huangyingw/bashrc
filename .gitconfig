[user]
	name = huangyingw@linux_programming
	email = huangyingw@gmail.com
[merge]
	tool = kdiff3
[diff]
	tool = kdiff3
[alias]
	ad = add
	bi = bisect
	br = branch
	bra = branch -a
	ci = commit -am
	cl = clone -v
	co = checkout
	ch = cherry
	chp = cherry-pick
	di = diff
	fe = fetch
	in = init
	lf = ls-files
	lg = log --pretty=oneline 
	me = merge
	met = mergetool
	pl = pull
	ps = push --all
	pst = push --tags
	rs = reset
	rsh = reset --hard
	rsm = reset --mixed
	rss = reset --soft
	rt = remote
	rtu = remote update
	rtv = remote -v
	rv = revert
	sm = submodule
  st = status
  sta = stash
  tg = tag -l -n1
  vd = difftool
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "dev"]
	remote = origin
	merge = refs/heads/dev
[branch "demo"]
	remote = origin
	merge = refs/heads/demo
[core]
	excludesfile = /root/.gitignore
